# Torexxx Agent â€” example environment variables (OpenRouter-only)
# Copy to .env or export in your shell.

# OpenRouter settings
# Use one of the keys below. If both are set, OPENROUTER_API_KEY takes precedence.
OPENROUTER_API_KEY=
# Embedded key for closed-source builds (used if OPENROUTER_API_KEY is empty)
OPENROUTER_EMBEDDED_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_REFERER=http://localhost
OPENROUTER_TITLE=Torexxx-Agent

# Model overrides (OpenRouter)
# Refine prompt model (JSON-friendly)
OR_MODEL_REFINE=google/gemma-3n-e2b-it:free
# Optional fallback for refine (used on 429/403 region)
OR_MODEL_REFINE_FALLBACK=mistralai/mistral-small:free
# Refine model pool (comma-separated, first is primary)
OR_MODELS_REFINE=qwen/qwen3-coder:free,mistralai/mistral-small:free
# Code generation model (primary)
OR_MODEL_CODEGEN=qwen/qwen3-coder:free
# Fallback model used if primary hits 429 rate limit
OR_MODEL_CODEGEN_FALLBACK=deepseek/deepseek-chat-v3.1:free
# Codegen model pool (comma-separated, first is primary)
OR_MODELS_CODEGEN=openai/gpt-oss-20b:free,moonshotai/kimi-k2:free
# Delay before retry on 429 (milliseconds)
OPENROUTER_RETRY_MS=2000

# YAML templates to apply (comma-separated names or paths)
AGENT_TEMPLATES=

# CLI loader settings
# Spinner style name from cli-spinners (e.g. dots, line, arc, bouncingBar)
AGENT_SPINNER_STYLE=dots
# Force ASCII-friendly spinner (true on Windows or dumb terminals)
AGENT_SPINNER_SIMPLE=false